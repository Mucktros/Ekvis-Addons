{
  "manifest_version": 3,
  "name": "Ekvis Addons",
  "version": "1.1",
  "description": "Adds Ekvis Addons To Ekvis",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://ekvis.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://ekvis.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
